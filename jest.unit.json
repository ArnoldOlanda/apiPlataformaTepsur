{
    "testEnvironmentOptions": {
        "jest": {
            "verbose": true,
            "extraGlobals": ["--develop"]
        }
    },
    "preset": "ts-jest",
    "coverageDirectory": "./coverage",
    "roots": ["./tests"],
    "testMatch": ["**/?(*.)+(spec).ts"],
    "resetMocks": true,
    "clearMocks": true
}
